class Node:
    def __init__(self,val):
        self.val=val
        self.next=None




class MyLinkedList:
    def __init__(self):
        self.head=None
        

    def get(self, index: int) -> int:
        x=0
        current=self.head
        while current:
            if x==index:
                return current.val
            current=current.next
            x+=1
        return -1
        
        
       
